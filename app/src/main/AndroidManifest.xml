<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kor.co.mu.jin.theword">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_MEDIA_LOCATION" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_daily_letter"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@mipmap/ic_daily_letter_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MYStroyWriteActivity"></activity>
        <activity android:name=".FamousItemClickActivity"></activity>
        <activity android:name=".DeveloperActivity" />
        <activity android:name=".popularItemClickActivity" />
        <activity android:name=".FragmentFamous" />
        <activity android:name=".GoodItemClickActivity" />
        <activity android:name=".FunnyItemClickActivity" />
        <activity android:name=".MYStoryItemClickActivity" />
        <activity android:name=".FragmentPopular" />
        <activity android:name=".SetProfileActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="f800b26613de776e0968d54d616eb0c4" />

        <service android:name=".SettingActivity"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
    </application>

</manifest>